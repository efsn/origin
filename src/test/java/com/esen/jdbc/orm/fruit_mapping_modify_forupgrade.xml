<?xml version="1.0" encoding="UTF-8"?>
<!-- 此文件用于测试 updateTable -->
<mapping>
	<entity name="orm.FruitModify" table="ES_FRUIT_MODIFY" bean="com.esen.jdbc.orm.FruitModify">		
		<properties>
			<!-- 修改数据类型, String 改为 int  -->
			<property name="id" field="ID_"  nullable="false" unique="true" length="20" pk="true" autoinc="false"/>
			<!-- 修改属性名和长度, produceArea 改为 area  -->
			<property name="nameNew" field="NAME_"  nullable="false" length="400" />
			<!-- 修改数据类型, 浮点型 改为 String  -->
			<property name="price" field="PRICE_"  nullable="true" length="20" />
			<!-- 修改属性名和字段名, produceArea 改为 area  -->
			<property name="area" field="AREA_" length="100"/>
			<!-- 删除属性 -->
			<!--property name="introduction" field="INTRODUCTION_" length="200" type="C" / -->
			<property name="saleDay" field="SALEDAY_" />
			<!-- 添加属性 -->
			<property name="addProp" field="ADDPROP_"  nullable="false" length="20" type="C"/>

			<!--  扩展字段全部删除 -->
		</properties>

		<indexes>
			<index name="INDEX_ID_NAME" fields="ID_,NAME_" unique="true"/>
		</indexes>

	</entity>
</mapping>