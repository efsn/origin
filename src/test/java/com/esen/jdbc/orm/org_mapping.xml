<?xml version="1.0" encoding="UTF-8"?>
<mapping>
	<entity name="orm.Org" table="ES_ORG" bean="com.esen.jdbc.orm.Org">
		<properties>
			<property name="orgId" field="ORGID_"  nullable="false" unique="true" length="20" pk="true" />
			<property name="name" field="NAME_"  nullable="false" length="100" />
			
			<property name="createDate" field="CREATEDATE_" />
			<property name="fromDate" field="FROMDATE_" />
			<property name="toDate" field="TODATE_" />
		</properties>
		
		<indexes>
			<index name="INDEX_ID_NAME" fields="ORGID_, FROMDATE_, TODATE_" unique="true"/>
		</indexes>
		
		<slowlychange fromdate="fromDate" todate="toDate" />
		
	</entity>
</mapping>